{   
    "source": {
        "name":"demo get data blogjs part 4",
        "url":"http://test.ceriwi.com/rule4/index.html",
        "remove-query": true,
        "remove-fragment": true,
        "browser": {
            "name" : "chrome",
            "host" : "http://chrome:4444/wd/hub",
            "setting": {
                "loadJavascript": true,
                "loadImages": false,
                "loadMediaStream": false,
                "loadStylesheets": false
            }
        }
    },
    "data": {
        "class":[
            {
                "name":"blog-main",
                "index":0,
                "child": {
                    "xpath":[
                        {
                            "name":"//h2[contains(@class, 'blog-post-title')]",
                            "alias":"title",
                            "index":0
                        },
                        {
                            "name":"//div[contains(@class,'blog-content')]",
                            "alias":"content",
                            "index":0
                        }
                    ]
                }
            }
        ]
    },
    "dataRebuild":{
        "title":"fix",
        "content":""
    }
}
